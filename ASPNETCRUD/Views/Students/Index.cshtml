@model List<ASPNETCRUD.Models.Student>
@{
}
<h1>Students</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            
            <th>Email</th>
            <th>Address</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                
                <td>@student.Email</td>
                <td>@student.Address</td>
                <td><a href="Students/View/@student.Id">View</a></td>
            </tr>
        }
    </tbody>
</table>
